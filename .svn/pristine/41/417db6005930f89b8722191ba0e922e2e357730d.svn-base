/*************************************
    select-group
*************************************/
.wf-select-group {
    position: relative;
    border-radius: 3px;
}

.wf-select-group::before {
    content: "\e942";
    font-family: 'xeicon';
    position: absolute;
    right: 10px;
    top: 50%;
    transform: translateY(-50%);
    font-size: 22px;
}

/*************************************
   인풋 스타일
*************************************/
.wf-select-group select {
    min-width: 200px;
}

.wf-select-group select,
.wf-input-group input,
[class*="wf-insert-form"] input,
[class*="wf-insert-form"] input[type="text"],
[class*="wf-insert-form"] input[type="date"],
[class*="wf-insert-form"] input[type="file"] {
    height: 40px;
    background: rgb(255 255 255 / 70%);
    border-radius: 5px;
    padding: 10px;
    font-size: 14px;
}

/* 모달 */
.modal .wf-select-group select,
.modal .wf-input-group input,
.modal [class*="wf-insert-form"] input,
.modal [class*="wf-insert-form"] input[type="text"],
.modal [class *="wf-insert-form"] input[type="date"],
.modal [class*="wf-insert-form"] input[type="file"],
.modal [class*="wf-insert-form"] textarea,
.modal .comment-area .input-wrap input {
