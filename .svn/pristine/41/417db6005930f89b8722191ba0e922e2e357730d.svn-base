}

.wf-menu-dropdown .alarm-list li .employee {
    font-size: 13px;
    font-weight: 700;
    color: #555;
}


/*************************************
    # sub
************************************/

.wf-main-container {
    display: flex;
    flex-direction: column;
    height: 100%;
}

.wf-tit-wrap {
    display: flex;
    justify-content: space-between;
    align-items: baseline;
    margin-bottom: 10px;
}

.wf-main-container .box-cont:not(.attend-cont) {
    background: #ffffffc2;

}

.wf-main-container .box-cont {
    height: calc(100% - 26px);
    border-radius: 7px;
}

.wf-content-wrap {
    flex: 1;
}

.page-tit {
    font-weight: 800;
    font-size: 25px;
    color: #002B19;
    display: flex;
    align-items: center;
    gap: 10px;
}

.page-tit2 {
