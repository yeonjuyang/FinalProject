@charset "UTF-8";
@import url(./webfonts.css);

/* 공통변수 */
:root {
    --main-color: #00a287;
    --main-color-grad: linear-gradient(to right, #06af93, #00a287);
    --warning-color: #ec4848;
    --color1: #b5f5cb;
    --color2: #abdbff;
    --color3: #fff0b6;
    --color4: #d6c6fd;
    --color5: #ffadc6;
    --gray-color: #f4f6f9;
    --border-radius: 14px;
    --glass-shadow: inset 0 0 0.12px rgba(255, 255, 255, 0.4),
        0 0 3px rgba(0, 0, 0, 0.01);
    --border: 1px solid rgba(255, 255, 255, 0.5);
    --placeholder: rgba(0, 0, 0, 0.254);
    --dropdown: 0px 0px 50px 0px rgba(82, 63, 105, 0.15);
    --glass-bg: rgba(255, 255, 255, 0.44);
    --glass-blur: blur(15px);
    --color-type1: #419c61;
    --color-type2: #5f7fef;
    --color-type3: #c6c6c6;
    --btn-shadow: 0 8px 16px rgba(0, 0, 0, 0.1);
}

/*************************************
    # reset
************************************/

* {
    padding: 0;
    margin: 0;
    outline: none;
    box-sizing: border-box;
    font-family: "Pretendard", sans-serif;
}

html,
body {
    font-family: "Pretendard", sans-serif;
    font-weight: 500;
    color: #333;
}

body {
    position: relative;
    padding: 30px;
    overflow: hidden;
    /* font-size: 13px; */
    font-size: 0.813rem;
    height: 100vh;
}

ul li {
    list-style: none;
}

a {
    text-decoration: none;
    color: inherit;
}

::selection {
    background: #2dffa134;
}

input {
    width: 100%;
    outline: none;
    border: none;
    font-family: "Pretendard", sans-serif;
}

input::placeholder {
    color: #8d8d8d;
}

.modal input::placeholder {
    color: #8d8d8d;
}

select,
textarea {
    width: 100%;
    appearance: none;
    outline: none;
    border: none;
}

input[type="checkbox"] {
    appearance: none;
    margin: 0;
    cursor: pointer;
}

button:disabled,
a:disabled {
    opacity: 0.2;
    cursor: default;
}

button {
    outline: none;
    border: none;
    appearance: none;
    cursor: pointer;
    background: transparent;
}


mark {
    display: block;
    background: none;
    font-size: 14px;
    font-weight: 600;
    color: #790bff;
    padding-bottom: 10px;
}

mark::before {
    content: "\e9aa";
    font-family: 'xeicon';
    font-size: 16px;
    display: inline-block;
    vertical-align: middle;
    color: #790bff;
}

i[class*="xi-"] {
    vertical-align: middle;
}

.xi-search {
    margin-bottom: 2px;
}

.xi-attachment {
    transform: rotate(322deg);
}

.txt-l {
    text-align: left;
}

.txt-r {
    text-align: right;
}

.txt-c {
    text-align: center;
}

.fs-sm {
    font-size: 12px !important;
}

/*************************************
    # 버튼스타일
************************************/
[class^="btn"] {
    padding: 10px 14px;
    border-radius: 5px;
    font-size: 15px;
    background: #F1F1F4;
    color: #252f4a;
    transition: all 0.3s;
}


[class^="btn"] i {
    font-size: 18px;
}

.btn1 {
    background: #F8FFF8;
    color: #252f4a;
}

.btn1:hover {
    color: #17C653;
}


.btn2 {
    background: #17C653;
    color: #fff;
}

.btn2:hover {
    background: #06752d;
}

.btn3 {
    background: #003A00;
    color: #fff;
}

.btn3:hover {
    background: #054905;
}

.btn4 {
    background: #375E9E;
    color: #fff;
}

.btn4:hover {
    background: #2a4d86;
}

.btn5 {
    background: #00a287;
    color: #fff;
}

.btn5:hover {
    background: #00816b;
}

.btn6 {
    background: transparent;
    color: #333;
}

.btn6:hover {
    text-decoration: underline;
}

.btn7 {
    background: #ff6b6b;
    color: #fff;
}

.btn7:hover {
    background: #f05454;
}

.w100 {
    width: 100% !important;
}

/*************************************
    # 폰트스타일
************************************/
.heading1 {
    font-size: 18px;
    font-weight: 700;
    margin-bottom: 15px;
}

.heading2 {
    font-size: 16px;
    font-weight: 700;
    margin-bottom: 15px;
}

.heading3 {
    font-size: 14px;
    font-weight: 700;
    margin-bottom: 10px;
}

.mg-l-5 {
    margin-left: 5px;
}

.mg-l-10 {
    margin-left: 10px;
}

.mg-r-5 {
    margin-right: 5px;
}

.mg-r-10 {
    margin-right: 10px;
}

.mg-b-10 {
    margin-bottom: 10px;
}

.txt-style1 {
    padding: 5px 10px;
    border-radius: 10px;
    background: var(--glass-bg);
}

/*************************************
    # 뱃지스타일
************************************/
[class*="wf-badge"] {
    display: inline-block;
    padding: 3px 13px;
    text-align: center;
    height: 20px;
    border-radius: 5px;
    font-size: 12px;
    font-weight: 600;
    background: #fff;
}

[class*="wf-badge"]+* {
    margin-left: 5px;
}

.wf-badge1 {
    color: #0cc24c;
    background: var(--color1);
}

.wf-badge2 {
    color: #319ef1;
    background: var(--color2);
}

.wf-badge3 {
    color: #ffa500;
    background: var(--color3);
}

.wf-badge4 {
    color: #9266ff;
    background: var(--color4);
}

.wf-badge5 {
    color: #f85385;
    background: var(--color5);
}

.i {
    color: #f8285a;
    font-weight: 900;
    margin: 0 3px;
    vertical-align: middle;
}

/*************************************
    # 상태스타일
************************************/
[class*="msg-txt"] {
    line-height: 2;
}

.msg-txt1 {
    /* 성공 메세지 */
    color: var(--main-color);
}

.msg-txt2 {
    /* 실패 메세지 */
    color: var(--warning-color);
}


/* ----------------------------------------- */

/*************************************
    # 로그인, 비번찾기
************************************/
body.login {
    padding: 0;
    height: 100vh;
}

.login-container {
    position: fixed;
    z-index: 999999;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    background: #82d7b6;
}

.login-wrap {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    display: flex;
    flex-direction: column;
    width: 450px;
    /* height: 450px; */
    padding: 53px 60px;
    border-radius: 15px;
    overflow: hidden;
    border: 2px solid rgba(255, 255, 255, 0.6);
    background: rgba(255, 255, 255, 0.2);
    box-shadow: inset 0 0 0.12px rgba(255, 255, 255, 0.4), 0 0 3px rgba(0, 0, 0, 0.01);
}

.login-wrap::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
    width: 100%;
    height: 100%;
    backdrop-filter: blur(8px);
}

.logo-wrap {
    margin: 0 auto 50px;
}

.logo-wrap .logo {
    color: var(--main-color);
    padding-left: 50px;
    position: relative;
}

.logo-wrap .logo::before {
    content: "";
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
    background: url(../img/favicon.png) center / cover;
    width: 40px;
    height: 40px;
}

.login-wrap .input {
    position: relative;
    border-radius: 10px;
}

.login-wrap .input::before {
    content: "";
    font-family: "xeicon";
    font-size: 1rem;
    font-weight: bold;
    color: var(--main-color);
    position: absolute;
    left: 18px;
    top: 50%;
    transform: translateY(-50%);
    z-index: 1;
}

.login-wrap .input.id::before {
    content: "\e9f3";
}

.login-wrap .input.pw::before {
    content: "\e964";
}

.login-wrap .input.mail::before {
    content: "\ea06";
}

.login-wrap .input input {
    position: relative;
    padding: 0 18px 0 45px;
    width: 100%;
    height: 40px;
    font-size: 14px;
    font-weight: 600;
    color: #333;
    border-radius: 10px;
    border: #fff;
    background: rgba(255, 255, 255, 0.5);
    box-shadow: inset 0 0 0.12px rgba(255, 255, 255, 0.4), 0 0 3px rgba(0, 0, 0, 0.01);
}

.login-wrap .input input::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
    width: 100%;
    height: 100%;
    backdrop-filter: blur(16px);
}

.login-wrap .input input::placeholder {
    font-size: 14px;
    font-weight: 400;
    color: #bdbdbd;
}

.login-wrap .input+.input {
    margin-top: 12px;
}

.login-wrap [class^="msg-txt"] {
    text-align: center;
}

.login-wrap .check-lst {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 16px;
    padding: 0 5px;
}

.login-wrap .check-lst .remember {
    display: flex;
    align-items: center;
    cursor: pointer;
}

.login-wrap .check-lst .remember .checkbox {
    position: relative;
    width: 18px;
    height: 18px;
    margin: 2px 5px 0 0;
    border-radius: 2px;
    background: #fff;
}

.login-wrap .check-lst .remember .checkbox:checked {
    background: var(--main-color);
}

.login-wrap .check-lst .remember .checkbox:checked::before {
    content: "\e929";
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    font-family: "xeicon";
    color: #fff;
}


.login-wrap .check-lst .remember label,
.login-wrap .check-lst .forgot {
    font-size: 14px;
    color: #5a5a5a;
    font-weight: 600;
    cursor: pointer;
}

.login-wrap .check-lst .forgot:hover {
    text-decoration: underline;
}

.login-wrap .submit-btn {
    display: block;
    margin: 60px auto 0;
    width: 160px;
    height: 46px;
    font-size: 16px;
    font-weight: 600;
    color: #fff;
    background: var(--main-color);
    box-shadow: var(--btn-shadow);
    border-radius: 10px;
    transition: all 0.3s;
}

.login-wrap .submit-btn:hover {
    color: #fff;
    background: var(--main-color);
}

.login-wrap .sub-tit {
    font-size: 18px;
    text-align: center;
    margin: -10px 0 20px;

}


/*************************************
    # 기본 레이아웃
************************************/

/* 퀵메뉴 */
.quick-menu-area {
    position: fixed;
    bottom: 40px;
    right: 40px;
    min-width: 120px;
}

.quickBtn {
    position: absolute;
    left: 73%;
    bottom: 0;
    transform: translateX(-50%);
    background: var(--main-color-grad);
    box-shadow: var(--btn-shadow);
    color: #fff;
    width: 60px;
    height: 60px;
    border-radius: 50%;
    font-size: 32px;
    display: flex;
    align-items: center;
    justify-content: center;
}

.active .quickBtn {
    background: rgba(255, 255, 255, 0.2);
    color: rgba(255, 255, 255, 0.8);
    backdrop-filter: blur(4px);
}

#glanceIcons {
    position: relative;
    bottom: 70px;
    color: #fff;
    display: flex;
    flex-direction: column;
    display: none;
    font-size: 16px;
    background: #ffffff;
    overflow: hidden;
    border-radius: 15px;
    box-shadow: 0px 0px 11px 3px rgb(101 121 118 / 37%);
}

#glanceIcons p {
    padding: 18px;
    background: #00a287;
    color: #ffffff;
    font-weight: 700;
    box-shadow: 0px 0px 11px 3px rgb(82 82 82 / 31%);
    transition: all 0.3s;
}

#glanceIcons p:hover {
    background: #007562;
}

#glanceIcons a {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 5px;
}

#glanceIcons i {
    font-size: 20px;
}

.active #glanceIcons {
    display: flex;
    /* opacity: 1;
    visibility: visible; */
}

/* 레이아웃 */
#gradient {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.main-container {
    display: flex;
    flex-direction: column;
    height: 100%;
    background: rgba(255, 255, 255, 0.2);
    box-shadow: var(--glass-shadow);
    border-radius: var(--border-radius);
    overflow: hidden;
    /* justify-content: center; */
    /* align-items: center; */
    /* min-height: calc(100vh - 52px); */
}

.custom {
    background: var(--glass-bg);
    /* backdrop-filter: blur(15px); */
    box-shadow: var(--glass-shadow);
    position: relative;
}

.custom::before {
    content: "";
    position: absolute;
    backdrop-filter: var(--glass-blur);
}

/* header */

header {
    display: flex !important;
    align-items: center;
    justify-content: space-between;
    flex: none;
    padding: 0 30px;
    width: 100%;
    height: 58px;
    border-bottom: var(--border);
    background: rgba(255, 255, 255, 0.44);
    position: sticky !important;
    top: 0;
    z-index: 10;
    border-top-right-radius: var(--border-radius);
}

header .logo {
    font-size: 20px;
    font-weight: 900;
    width: calc(260px - 30px);
    color: #0c463c;
    position: relative;
    padding-left: 42px;
    padding-top: 4px;
    letter-spacing: 1px;
}

header .logo::before {
    content: "";
    background: url(../img/favicon.png) no-repeat center /cover;
    width: 30px;
    height: 30px;
    position: absolute;
    top: 50%;
    left: 0;
    transform: translate(0, -50%);
}

.search-bar {
    height: 35px;
    display: flex;
    width: 500px;
    border-radius: 5px;
    background: rgba(255, 255, 255, 0.65);
    backdrop-filter: blur(5px);
    margin: 0 auto 0 30px;
    position: relative;
}

.search-bar::before {
    content: "\e97a";
    font-family: 'xeicon';
    font-size: 1rem;
    color: var(--placeholder);
    position: absolute;
    top: 50%;
    left: 10px;
    transform: translateY(-50%);
}

.search-bar input {
    width: 100%;
    height: 100%;
    background-color: var(--search-bg);
    font-size: 15px;
    padding: 0 20px 0 35px;
    box-shadow: 0 0 0 2px rgba(134, 140, 160, 0.02);
    font-weight: 700;
    color: #000;
}


.search-bar input::placeholder {
    font-size: 15px;
    font-weight: 500;
    color: var(--placeholder);
}

header .header-txt {
    margin: 0 auto 0 30px;
    font-weight: 700;
    color: #151515;
    font-size: 16px;
}

.wf-util,
.side-util {
    display: flex;
    align-items: center;
    justify-content: center;
}

.wf-util>li,
.side-util>li {
    position: relative;
    margin-left: 15px;
}

.wf-util>li>a,
.side-util>li>a {
    position: relative;
    display: inline-block;
}

.wf-util>li>a i,
.side-util>li>a i {
    display: inline-block;
    font-size: 1.5rem;
    color: #375E9E;
}

.wf-util button+button,
.side-util button+button {
    margin-left: 10px;
}

button:hover i,
a:hover i {
    animation: swing ease-in-out 0.5s 1 alternate;
}

@keyframes swing {

    0%,
    30%,
    50%,
    70%,
    100% {
        transform: rotate(0deg);
    }

    10% {
        transform: rotate(10deg);
    }

    40% {
        transform: rotate(-10deg);
    }

    60% {
        transform: rotate(5deg);
    }

    80% {
        transform: rotate(-5deg);
    }
}


.wf-util>li .user-img-icon {
    width: 40px;
    height: 40px;
    border-radius: 50%;
    overflow: hidden;
    background: #fff;
}

.wf-util>li .user-img-icon .user-img {
    width: 100%;
    height: 100%;
}

/*************************************
    # main
************************************/
main {
    flex: 1;
    width: 100%;
    padding: 30px;
    height: 100%;
    overflow: auto;
    overflow-x: hidden;
}

main .wf-box-wrap {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    grid-template-rows: repeat(3, 1fr);
    gap: 20px;
    /* height: 100%; */
    height: calc(100vh - 180px);
}

main .wf-box-wrap .wf-box {
    width: 100%;
    min-height: 234px;
    padding: 20px;
    border-radius: var(--border-radius);
    border: var(--border);
    box-shadow: 0 6px 15px rgba(36, 37, 38, 0.08);
    overflow: hidden;
}

main .wf-box-wrap .wf-box .wf-box-tit {
    font-size: 18px;
    font-weight: 800;
    color: #333;
    display: flex;
    align-items: center;
}

main .wf-box-wrap .draggable {
    cursor: grab;
}

main .wf-box-wrap .draggable.dragging {
    opacity: 0.5;
}


.wf-main-container .go-btn {
    display: flex;
    align-items: center;
    gap: 4px;
    color: rgba(0, 0, 0, 0.522);
}

.wf-main-container .go-btn:hover::after {
    transform: translateX(3px);
}

.wf-main-container .go-btn::after {
    content: "\e93e";
    font-family: 'xeicon';
    font-size: 12px;
    display: block;
    color: #00a287;
    transition: all 0.3s;
}


.wf-main-container .tit-wrap {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 10px;
}

.wf-main-container .tit-wrap .tit-wrap-inner {
    display: flex;
    align-items: center;
}

.wf-main-container .tit-wrap .tit-wrap-inner .txt {
    margin-left: 3px;
}

.wf-main-container .tit-wrap .cnt {
    margin-left: 5px;
    padding: 1px 10px;
    background: #00a287;
    border-radius: 7px;
    font-size: 12px;
    font-weight: 700;
    color: #fff;
}


/* box-list1 */
.box-list1 {
    max-height: calc(100% - 32px);
    overflow-y: auto;
}

.box-list1>li>a:hover {
    background: #fff;
}

.box-list1>li>a {
    display: flex;
    align-items: center;
    font-size: 14px;
    font-weight: 600;
    padding: 5px 7px;
    border-radius: 5px;
    background: #ffffffb4;
    gap: 7px;
    transition: all 0.3s;
}

.box-list1>li>a.noData {
    justify-content: center;
    height: 160px;
    background: #6565650f;
}

.box-list1>li+li {
    margin-top: 5px;
}

.box-list2 {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 10px;
    text-align: center;
}

.box-list2>li {
    background: #ffffffd1;
    padding: 8px;
    border-radius: 8px;
}

.box-list2>li>a {
    width: 100%;
    display: flex;
    flex-direction: column;
    gap: 6px;
    min-height: 54px;
}

.box-list3 {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 10px;
    text-align: center;
    height: calc(100% - 26px);
    overflow-y: auto;
}

.box-list3>li {
    background: #ffffffd1;
    border-radius: 8px;
}

.box-list3>li .tit {
    -webkit-line-clamp: 3;
}

.box-list3>li>a {
    width: 100%;
    display: flex;
    flex-direction: column;
    gap: 10px;
    padding: 8px;
    align-items: center;
    height: 100%;
    border-radius: 8px;
}

.box-list3>li>a:hover {
    background: #fff;
}


.box-list3>li>a .top {
    margin-top: auto;
}

.box-list3>li>a .emp {
    display: block;
}

.box-list3>li>a .date {
    display: inline-block;
    margin-top: 5px;
}

.wf-box .emp {
    font-weight: 700;
    font-size: 12px;
}

.wf-box .tit {
    max-width: 97%;
    text-transform: capitalize;
    overflow: hidden;
    white-space: normal;
    text-overflow: ellipsis;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    display: -webkit-box;
    font-weight: 700;
}

.wf-box .box-list2 .tit {
    -webkit-line-clamp: 2;
}

.wf-box .date {
    color: #a5a5a5;
    font-weight: 500;
    flex: none;
    font-size: 13px;
    margin-left: auto;
}

.wf-box .img-wrap {
    width: 40px;
    height: 40px;
    border-radius: 50%;
    background: #c9e2cb;
    overflow: hidden;
    flex: none;
}

.wf-box .img-wrap img {
    width: 100%;
    height: 100%;
}

.wf-box .emp-wrap {
    display: flex;
    flex-direction: column;
    gap: 3px;
    width: 100%;
}

.wf-box .badge {
    padding: 5px 7px;
    border-radius: 6px;
    font-size: 15px;
    font-weight: 600;
    line-height: 1;
    font-size: 12px;
    flex: none;
}

.wf-box .badge i {
    font-size: 14px;
}

.wf-box .badge.badge1 {
    color: #9266ff;
    background: var(--color4);
}

.wf-box .badge.badge2 {
    color: #ffa500;
    background: var(--color3);
}

.wf-box .badge.badge3 {
    color: #319ef1;
    background: var(--color2);
}

.wf-box .badge.badge4 {
    color: #0cc24c;
    background: var(--color1);
