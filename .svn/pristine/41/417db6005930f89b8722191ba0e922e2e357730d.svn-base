/*************************************
    # modal
************************************/

.scroll-hidden {
    overflow: hidden;
    height: 100%;
}

.modal {
    opacity: 0;
    visibility: hidden;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background: rgba(0, 0, 0, 0.6);
    /* backdrop-filter: blur(20px); */
    transition: all 0.3s;
    z-index: 99999;
}

.modal .modal-cont {
    opacity: 0;
    visibility: hidden;
    position: relative;
    display: flex;
    flex-direction: column;
    min-width: 660px;
    max-width: 1200px;
    background: #fff;
    border-radius: 25px;
    padding: 40px;
    max-height: 90%;
    /* overflow: auto; */
}

/* modal open */
.modal.open {
    opacity: 1;
    visibility: visible;
}

.modal.open .modal-cont {
    opacity: 1;
    visibility: visible;
    animation: appear 500ms;
}

@keyframes appear {
    0% {
        opacity: 0;
        transform: translateY(-20px);
    }
}

/* 모달 제목 */
.modal-tit {
    font-size: 25px;
    font-weight: 700;
}

.modal .close-btn {
    position: absolute;
    top: 38px;
    right: 38px;
}

/* 버튼 영역 */
.modal .modal-btn-wrap {
    text-align: center;
}

.modal .modal-btn-wrap [class^="btn"] {
    min-width: 100px;
}

.modal .modal-btn-wrap [class^="btn"]+[class^="btn"] {
    margin-left: 3px;
}

.modal .close-btn::before {
    font-size: 26px;
    content: "\e921";
    font-family: 'xeicon';
}

.modal .close-btn:hover {
    background: none;
}

/* 모달 컨텐츠 영역에서 스크롤 생성 */
.modal-content-area {
    padding: 25px 0 30px;
    max-height: calc(100% - 50px);
    overflow-y: auto;
    overflow-x: hidden;
}

/*************************************
    # scroll bar
************************************/
.modal .modal-content-area::-webkit-scrollbar {
    width: 0.9rem;
    padding: 0 0.5rem;
    background: transparent;
}

.modal .modal-content-area::-webkit-scrollbar-thumb {
    background-color: rgba(126, 126, 126, 0.088);
    border-radius: 4rem;
    background-clip: padding-box;
    border: 0.33rem solid transparent;
}

.modal .modal-content-area::-webkit-scrollbar-thumb:hover {
    background-color: rgba(56, 56, 56, 0.088);
}

.modal .modal-content-area::-webkit-scrollbar-track {
    background: transparent;
    border-radius: 0;
}

/*************************************
    # ck-editor
************************************/
.ck.ck-editor {
    width: 100% !important;
}

.ck.ck-content {
    height: 400px;
}

