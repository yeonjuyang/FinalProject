}

.wf-flex-box .wf-content-area+.wf-content-area {
    margin-top: 0;
}

.wf-content-area+.wf-content-area {
    margin-top: 20px;
}

/* wf-content-box */
.box2 .wf-content-box {
    flex: 0 1 calc(100% / 2);
}

.box3 .wf-content-box {
    flex: 0 1 calc(100% / 3);
}

.box4 .wf-content-box {
    flex: 0 1 calc(100% / 4);
}

.wf-content-box {
    flex: 0 1 calc(100% / 5);
