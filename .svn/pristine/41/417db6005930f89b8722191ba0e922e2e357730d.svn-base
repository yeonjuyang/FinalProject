/* 검토자 툴팁 */
.middle-tooltip {
    width: max-content;
    position: fixed;
    transform: translate(-50%, -50%);
    top: 30%;
    left: 58.6%;
    background: #5f7fef;
    border-radius: 7px;
    padding: 20px;
    z-index: 100;
    text-align: center;
    color: #fff;
    box-shadow: 2px 2px 14px 0px rgb(34 54 87 / 47%);
    opacity: 0;
    visibility: hidden;
    transition: top 0.5s;
}

.middle-tooltip::after {
    content: "";
    position: absolute;
    top: 100%;
    left: 50%;
    margin-left: -10px;
    border-width: 10px;
    border-style: solid;
    border-color: #5f7fef transparent transparent transparent;
}

.middle-tooltip.active {
    opacity: 1;
    visibility: visible;
    top: 28%;
}

.middle-tooltip .middle-wrap {
    border-top: 1px solid #ffffff9e;
    padding-top: 10px;
}

.middle-tooltip .middle-wrap li {
    padding: 5px;
    display: flex;
    align-items: center;
    gap: 10px;
    position: relative;
}

.middle-tooltip .middle-wrap li+li {
    margin-top: 10px;
}

.middle-tooltip .middle-wrap li+li:before {
    content: "\e941";
    font-family: 'xeicon';
    font-size: 17px;
    color: #fff;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    top: -14px;
    opacity: 0.7;
}

.middle-tooltip .middle-wrap li .num {
    font-size: 25px;
    font-weight: 800;
    margin-right: 5px;
}

.middle-tooltip .middle-wrap .img-wrap {
    width: 33px;
    height: 33px;
    border-radius: 50%;
    background: #fff;
    overflow: hidden;
    flex: none;
}

.middle-tooltip .middle-wrap .img-wrap img {
    width: 100%;
    height: 100%;
}

.middle-tooltip .middle-wrap .emp {
    font-size: 15px;
    font-weight: 700;
}

.middle-tooltip [class*="wf-badge"] {
    margin-left: auto;
}

/* 전자서명 */
.sign-cont {
    position: relative;
    height: 193px;
    aspect-ratio: 1;
}

.sign-cont::after {
    content: "";
    background: url(/resources/img/approval/pen.svg) no-repeat center / cover;
    position: absolute;
    top: 14%;
    right: -169px;
    width: 100px;
    height: 100px;
    transform: scale(1.2);
    opacity: 0.6;
    animation: penMovement 2s infinite alternate;
}

@keyframes penMovement {
    0% {
        transform: scale(1.2) rotate(0deg);
    }

    25% {
        transform: scale(1.2) rotate(10deg);
    }

    50% {
        transform: scale(1.2) rotate(0deg);
