/*************************************
   wf-insert-form
*************************************/

/* 등록 */
[class^="wf-insert-form"] {
    padding: 25px;
}

.modal [class^="wf-insert-form"] {
    padding: 0;
}

.wf-insert-form {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: 15px;
}

[class^="wf-insert-form"].vertical {
    grid-template-columns: repeat(1, 1fr);
}

[class^="wf-insert-form"]>li,
[class^="wf-insert-form"]>div {
    display: flex;
    align-items: center;
}


.wf-insert-form label,
[class^="wf-insert-form"] label {
    width: 130px;
    flex: none;
}

.modal [class^="wf-insert-form"] label,
.modal [class^="wf-insert-form"] label {
    width: 200px;
}

[class^="wf-insert-form"]>li>label,
[class^="wf-insert-form"] label {
    display: inline-block;
    font-size: 15px;
    font-weight: 700;
}

.wf-insert-form>li>div {
    flex: 0 1 360px;
}

/* 상세 */
.wf-insert-form.detail {
    gap: 25px;
}

.wf-insert-form2.detail {
    gap: 25px 30px;
}

[class^="wf-insert-form"].detail,
[class^="wf-insert-form"].detail>li>label {
    font-size: 16px;
    font-weight: 600;
}

[class^="wf-insert-form"].detail>li>label {
    font-weight: 700;
    color: #375e9e;
}

[class^="wf-insert-form"] .img-box {
    width: 100px;
    height: 100px;
    border-radius: 50%;
    overflow: hidden;
    background: #fff;
    flex: none;
}


[class^="wf-insert-form"] .img-box img {
    width: 100%;
    height: 100%;
}

.modal [class^="wf-insert-form"] .img-box img {
    background: #efefef;
}

