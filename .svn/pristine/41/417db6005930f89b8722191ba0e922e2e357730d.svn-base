    height: 50px;
    border-radius: 50%;
    background: #ddd;
}

.wf-menu-dropdown .user-info {
    display: block;
    font-size: 13px;
}

.wf-menu-dropdown .user-name {
    display: block;
    font-weight: 800;
    padding-top: 3px;
}

.wf-menu-dropdown .alarm-list {
