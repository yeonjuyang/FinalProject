}


/* 차트 영역 */
.wf-chart {
    background: rgba(255, 255, 255, 0.25);
    color: #0da242;
    padding: 10px;
}

.center-table th,
.center-table td {
    text-align: center !important;
}

/* table */
.wf-table {
    border-collapse: collapse;
    width: 100%;
    font-size: 14px;
    font-weight: 600;
    background: var(--glass-bg);
    border-radius: 5px;
    overflow: hidden;
}

.wf-table th,
.wf-table td {
    padding: 10px 20px;
    text-align: left;
    border-bottom: 1px solid rgba(255, 255, 255, 0.42);
    white-space: nowrap;
}

.wf-table th {
    color: #545454;
    background: #f9f9f971;
    white-space: nowrap;
}

.wf-table td {
    color: #444;
    padding: 12px 20px;
}

.read-table.wf-table tr:hover td {
    cursor: default;
}

.wf-table tr.default-cursor:hover td {
    cursor: default;
}

.wf-table tr:hover td {
    cursor: pointer;
    background: #ffffff74;
}

.wf-table td:has(button, a) {
    padding: 6px 20px;
}

.wf-table td button,
.wf-table td a,
.wf-table td [class*="btn"] {
    padding: 5px 20px;
    position: relative;
    z-index: 100;
    pointer-events: auto;
    font-weight: 500;
}

.wf-table td button+button,
.wf-table td a+a {
    margin-left: 2px;
}

.center.th {
    text-align: center;
}

.center.td {
    text-align: center;
}

/* 1줄로 말줄임 */
.wf-table td>p {
    text-transform: capitalize;
    overflow: hidden;
    white-space: normal;
    text-overflow: ellipsis;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    display: -webkit-box;
}

.wf-search-area {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 10px;
    width: 500px;
    margin: 20px auto 0;
}

.wf-search-area .select-box {
    position: relative;
    background: var(--glass-bg);
    backdrop-filter: var(--glass-blur);
    border-radius: 5px;
    width: 150px;
}

.wf-search-area .select-box::before {
    content: "\e942";
    font-family: 'xeicon';
    position: absolute;
    right: 10px;
    top: 50%;
    transform: translateY(-50%);
    font-size: 21px;
}

.wf-search-area select,
.wf-search-area input {
    height: 40px;
    padding: 0 15px;
    border-radius: 5px;
    background: inherit;
}

.wf-search-area input {
    background: var(--glass-bg);
    backdrop-filter: var(--glass-blur);
}

.wf-search-area select:focus,
.wf-search-area input:focus {
    background: #fff;
}

.wf-search-area button {
    height: 40px;
    min-width: 50px;
}

/* 페이지네이션 */
.wf-pagination-wrap {
    width: fit-content;
    margin: 30px auto 0;
}

.wf-pagination-wrap .pagination {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 6px;
}

.wf-pagination-wrap .pagination .page-item {
    width: 30px;
    height: 30px;
    border-radius: 4px;
    background: #ffffff88;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #38473c;
    cursor: pointer;
    font-size: 14px;
}

.wf-pagination-wrap .pagination .page-item:hover {
    background: rgba(4, 206, 108, 0.115);
}

.wf-pagination-wrap .pagination .page-item.active {
    border: 1px solid var(--main-color);
    background: rgba(4, 206, 108, 0.318);
}

.wf-pagination-wrap .pagination .frst,
.wf-pagination-wrap .pagination .last {
    position: relative;
}

.wf-pagination-wrap .pagination .frst::before,
.wf-pagination-wrap .pagination .frst::after,
.wf-pagination-wrap .pagination .last::before,
.wf-pagination-wrap .pagination .last::after {
    content: "";
    font-family: 'xeicon';
    font-size: 13px;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.wf-pagination-wrap .pagination .frst::before {
    content: "\e93b";
    left: 3px;
}

.wf-pagination-wrap .pagination .frst::after {
    content: "\e93b";
    left: -2px;
}


.wf-pagination-wrap .pagination .last::before {
    content: "\e93e";
    left: 3px;
}

.wf-pagination-wrap .pagination .last::after {
    content: "\e93e";
    left: -2px;
}

/*************************************
    # 업무, 프로젝트
************************************/

.wf-list-style>li {
    position: relative;
    padding-left: 10px;
    font-size: 14px;
    text-transform: capitalize;
    overflow: hidden;
    white-space: normal;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    display: -webkit-box;
}


.wf-list-style>li+li {
    margin-top: 10px;
}

.wf-list-style>li::before {
    content: "";
    width: 3px;
    border-radius: 3px;
    height: 100%;
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
    background: #ddd;
}

.wf-list-style>.style1::before {
    background: var(--color1);
}

.wf-list-style>.style2::before {
    background: var(--color2);
}

.wf-list-style>.style3::before {
    background: var(--color3);
}

.wf-list-style>.style4::before {
    background: var(--color4);
}

.wf-list-style>.style5::before {
    background: var(--color5);
}

/* wf-work-detail-info */
.wf-work-detail-info {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.wf-worker .img-lst {
    display: flex;
    align-items: center;
    gap: 6px;
}

.wf-worker .img-wrap {
    width: 40px;
    height: 40px;
    border-radius: 50%;
    overflow: hidden;
    background: #eeeeee;
}

.wf-worker .img-lst img {
    width: 100%;
    height: 100%;
}

.wf-worker .txt {
    font-size: 15px;
