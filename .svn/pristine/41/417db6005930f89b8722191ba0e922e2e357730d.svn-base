/* 완료 */
.completed {}

.timeline>li.completed .icon::before {}

/* 진행중 */
.timeline>li.wf-progress .icon {
    transform: scale(1.4);
}

.timeline>li.wf-progress .icon::before {
    content: "\e9c3";
}

.timeline>li.wf-progress .icon {
    background: var(--color-type2);
}

.timeline>li.wf-progress .icon::after {
    content: "";
    width: 5px;
    height: 5px;
    border-radius: 50%;
    position: absolute;
    top: -2px;
    right: -2px;
    background: var(--color-type2);
    animation: blink 1.5s infinite;
}

@keyframes blink {
    0% {
        opacity: 1;
    }

    50% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}


.timeline>li.wf-progress .name {
    color: var(--color-type2);
}

