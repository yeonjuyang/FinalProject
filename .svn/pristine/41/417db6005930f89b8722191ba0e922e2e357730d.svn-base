/*************************************
    # context-menu
************************************/
.context-menu {
    visibility: hidden;
    position: absolute;
    right: 0;
    top: 31px;
    z-index: 1;
    width: 150px;
    padding: 4px;
    border-radius: 5px;
    background: #fff;
    box-shadow: 0px 12px 20px 4px rgba(0, 0, 0, 0.02);
}

.context-menu button {
    display: block;
    width: 100%;
    padding: 4px 10px;
    border-radius: 3px;
    text-align: left;
    font-size: 14px;
    color: #808080;
    font-weight: 500;
}

.context-menu button::before {
    content: "";
    font-family: 'xeicon';
    font-size: 1rem;
    font-weight: bold;
    margin-right: 10px;
}

.context-menu button.edit::before {
    content: "\e9c4";
}

.context-menu button.delete::before {
    content: "\e96e";
}

.context-menu button.upload::before {
    content: "\f03e";
}

.context-menu button:not(.delete):hover {
    background: #f2f2f2;
}

.context-menu .delete:hover {
    background: #fcf3f4;
    color: #FE415E;
}

.context-menu.active {
    visibility: visible;
}

/*************************************
    # tab
************************************/

/* tab-type1 */
.tab-type1 .tab-menu {
    position: relative;
    display: inline-flex;
    align-items: center;
    background: rgb(255 255 255 / 73%);
    border-radius: 10px;
    margin-bottom: 7px;
}

.tab-type1 .tab-menu .tab-btn {
    display: block;
    padding: 7px 10px;
    font-size: 14px;
}

.tab-type1 .tab-menu .tab-btn.active {
    color: #000;
    font-weight: 600;
    transition-delay: 0.1s;
}

.tab-type1 .tab-content {
    display: none;
}

.tab-type1 .tab-content.active {
    display: block;
}

.tab-type1 .tab-indicator {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 0;
    height: 100%;
    border: 2px solid var(--main-color);
    background: rgb(19 186 124 / 22%);
    border-radius: 10px;
    cursor: pointer;
    transition: transform 0.2s ease-in-out, width 0.2s ease-in-out;
}

.tab-type1 .tab-menu .tab-btn,
.tab-type1 .tab-indicator {
    min-width: 140px;
}

/* tab-type2 */
.tab-type2>.tab-menu {
    position: relative;
    display: flex;
    align-items: center;
    border-bottom: 3px solid rgba(255, 255, 255, 0.5);
}

.tab-type2>.tab-menu::before {
    content: "";
    border-bottom: 3px solid rgba(255, 255, 255, 0.5);
    width: 50px;
    position: absolute;
    z-index: -1;
    bottom: -3px;
    left: -50px;
}

.tab-type2>.tab-menu::after {
    content: "";
    border-bottom: 3px solid rgba(255, 255, 255, 0.5);
    width: 50px;
    position: absolute;
    z-index: -1;
    bottom: -3px;
    right: -50px;
}

.tab-type2>.tab-menu .tab-btn {
    display: block;
    margin: 0 20px;
    padding: 5px 0 10px;
    font-size: 15px;
    font-weight: 500;
    color: rgb(109 109 109 / 79%);
    transition: color 0.1s;
}

.tab-type2>.tab-menu .tab-btn:first-of-type {
    margin: 0 20px 0 0;
}

.tab-type2>.tab-menu .tab-btn.active {
    color: var(--color-dark);
    font-weight: 800;
    transition-delay: 0.1s;
}

.tab-type2 .tab-content {
    display: none;
    padding: 20px 0;
}

.tab-type2 .tab-content.active {
    display: block;
}

.tab-type2 .tab-indicator {
    position: absolute;
    bottom: -3px;
    left: 0;
    width: 0;
    height: 100%;
    border-bottom: 3px solid var(--main-color);
    cursor: pointer;
    transition: transform 0.2s ease-in-out, width 0.2s ease-in-out;
}

.tab-type2 .tab-indicator {
    min-width: 20px;
    transform: translateX(0px);
}

.tab-board-lst {
    padding-top: 10px;
}

.tab-type1 .tab-content.active .tab-board-lst,
.tab-type2 .tab-content.active .tab-board-lst,
.tab-type3 .tab-content.active .tab-board-lst {
    animation: fade-in-bottom 0.5s;
}

@keyframes fade-in-bottom {
    0% {
        opacity: 0;
        transform: translateY(15px);
    }

    100% {
        opacity: 1;
        transform: translateY(0);
    }
}

/* tab-type3 */
.tab-type .cnt {
