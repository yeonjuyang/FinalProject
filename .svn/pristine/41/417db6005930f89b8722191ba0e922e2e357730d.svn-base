}

.wf-menu-dropdown {
    box-shadow: var(--dropdown);
    padding: 20px;
    border-radius: 15px;
    position: absolute;
    top: 40px;
    right: -20px;
    min-width: 240px;
    background: #fff;
    z-index: 12;
    opacity: 0;
    visibility: hidden;
    transition: opacity 0.3s, transform 0.3s;
    transform: translateY(-20px);
}

.wf-menu-open .wf-menu-dropdown {
    opacity: 1;
    visibility: visible;
    transform: translateY(0);
